{
  "name": "@twipped/hooks",
  "version": "0.4.0",
  "description": "A collection of my frequently used react hooks",
  "engines": {
    "node": ">=14.13"
  },
  "type": "module",
  "main": "./index.js",
  "exports": {
    ".": "./index.js",
    "./*": "./hooks/*.js"
  },
  "scripts": {
    "lint": "eslint './**/*.{js,jsx}' --fix",
    "test": "tap",
    "doc": "jsdoc -c jsdocs.json",
    "docs": "jsdoc2md -t README.hbs hooks/*.js > README.md",
    "release": "semantic-release"
  },
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Twipped/hooks"
  },
  "release": {
    "preset": "conventionalcommits",
    "branches": [
      "main"
    ],
    "plugins": [
      "@semantic-release/commit-analyzer",
      "@semantic-release/release-notes-generator",
      [
        "@semantic-release/changelog",
        {
          "changelogFile": "CHANGELOG.md"
        }
      ],
      "@semantic-release/npm",
      [
        "@semantic-release/git",
        {
          "assets": [
            "CHANGELOG.md",
            "README.md",
            "package.json"
          ]
        }
      ],
      "@semantic-release/github"
    ]
  },
  "author": "Jocelyn Badgley <joc@twipped.com> (http://twipped.com)",
  "license": "MIT",
  "files": [
    "hooks",
    "CHANGELOG.md",
    "LICENSE.txt",
    "README.md",
    "index.js"
  ],
  "dependencies": {
    "@twipped/utils": "~5.0.2"
  },
  "peerDependencies": {
    "prop-types": ">=15",
    "react": ">=17"
  },
  "optionalDependencies": {
    "@emotion/css": ">=11.7.1",
    "@emotion/react": ">=11.7.1",
    "date-fns": ">=2.28.0",
    "mobx": ">=6.3",
    "mobx-react-lite": ">=3.2.3",
    "qs": ">=6.10.3"
  },
  "devDependencies": {
    "@babel/eslint-parser": "^7.17.0",
    "@babel/preset-env": "~7.16.8",
    "@babel/preset-react": "~7.16.7",
    "@semantic-release/changelog": "^6.0.1",
    "@semantic-release/git": "^10.0.1",
    "@semantic-release/github": "^8.0.2",
    "@testing-library/react-hooks": "^7.0.2",
    "enzyme": "^3.11.0",
    "eslint": "~8.7.0",
    "eslint-config-twipped": "~6.1.0",
    "eslint-plugin-import": "~2.25.4",
    "eslint-plugin-jsdoc": "~37.9.4",
    "eslint-plugin-node": "~11.1.0",
    "eslint-plugin-promise": "~6.0.0",
    "eslint-plugin-react": "~7.28.0",
    "jsdoc": "~3.6.10",
    "jsdoc-to-markdown": "~7.1.1",
    "minami": "~1.2.3",
    "react": ">=17",
    "react-test-renderer": "^17.0.2",
    "semantic-release": "^19.0.2",
    "tap": "^16.0.0"
  }
}
